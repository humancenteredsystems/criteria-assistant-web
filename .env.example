# Environment variables for Criteria Assistant Web
# Copy this file to .env.local for local development

# Application Configuration
VITE_APP_NAME=Criteria Assistant Web
VITE_APP_VERSION=1.0.0

# PDF.js Configuration
VITE_PDF_WORKER_URL=/pdf.worker.min.js

# Feature Flags (for future phases)
VITE_ENABLE_ANNOTATIONS=true
VITE_ENABLE_SEARCH=true
VITE_ENABLE_EXPORT=true

# Analytics (optional - for future use)
# VITE_ANALYTICS_ID=your-analytics-id

# API Configuration (for future server integration)
# VITE_API_BASE_URL=https://api.example.com
